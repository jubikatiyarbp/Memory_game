{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DATABASE_CONNECTION_STRING"
  },
  "JwtSettings": {
    "SecretKey": "YOUR_SUPER_SECRET_KEY",
    "Issuer": "http://localhost:<YOUR_APPLICATION_PORT>",
    "Audience": "http://localhost:5500",
    "ExpiryMinutes": 20
  }
}
